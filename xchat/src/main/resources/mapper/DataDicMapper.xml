		 values(

			#{code,jdbcType=VARCHAR},
			#{value,jdbcType=VARCHAR},
			#{id,jdbcType=INTEGER})
	</insert>
	<!-- 插入记录 -->
	<insert id="insertSelective" parameterType="xchat.pojo.DataDic" >
				insert into data_dic
	<trim  prefix="(" suffix=")"  suffixOverrides="," >
	<if test="code != null  ">
