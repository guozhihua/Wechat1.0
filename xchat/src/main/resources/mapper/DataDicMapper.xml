	</trim>
	</insert>
	<!-- 修改记录，只修改只不为空的字段 -->
	<update id="updateByPrimaryKeySelective" parameterType="xchat.pojo.DataDic" >
		update data_dic set 
	<trim  suffixOverrides="," >
	<if test="value != null ">
		value=#{value,jdbcType=VARCHAR},
	</if>
	</trim> where code=#{code,jdbcType=VARCHAR}
